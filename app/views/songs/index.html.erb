<ul>
<% @songs.each do |song| %>
<h1><%= link_to song.title, song_path(song) %></h1>
<% if song.artist.present? %>
<h1><%= link_to song.artist.name, artist_path(song.artist) %></h1>
<% else %>
<h1><%= link_to "Add Artist", edit_song_path(song) %></h1> 
<% end %>
<% end %>
</ul>
<br>
<%= link_to "Add New Song", new_song_path %>
